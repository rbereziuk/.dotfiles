{
  "require": {
    "prefix": "rq1",
    "body": [
      "const ${1:module} = require('${2:$1}')"
    ],
    "description": "Require module for Node.js"
  },
  "createElement": {
    "prefix": "cel",
    "body": "const ${1:el} = ${2:document}.createElement('${3:elem}');${0}"
  },
  "Node callback": {
    "prefix": "cb",
    "body": "function (err, ${1:value}) {\n\tif (err) throw err\n\t${0}\n}"
  },
  "Route callback": {
    "prefix": "rcb",
    "body": [
      "(req, res) => {",
      "  ${0}",
      "}"
    ]
  },
  "Log in console": {
    "prefix": "cl",
    "body": "console.log('${1:emoji}', ${2:value})${0}"
  },
  "Switch Statement": {
    "prefix": "switch",
    "body": [
      "switch (${1:key}) {",
      "\tcase ${2:value}:",
      "\t\t$0",
      "\t\tbreak;",
      "",
      "\tdefault:",
      "\t\tbreak;",
      "}"
    ],
    "description": "Switch Statement"
  },
  "import": {
    "prefix": "im",
    "body": "import { $2 } from '${1:package or file}';$0",
    "description": "Basic js import"
  },
  "default import": {
    "prefix": "imd",
    "body": [
      "import $2 from '${1:package or file}';",
      "$0"
    ],
    "description": "Basic dafault js import"
  },
  "import witn new line": {
    "prefix": "imln",
    "body": [
      "import $2 from '${1:package or file}';",
      "$0"
    ],
    "description": "Basic js import with new line"
  }
}
