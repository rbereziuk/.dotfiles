{
  "name": "snippets-in-json",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": [
          "javascript",
          "javascriptreact",
          "typescript",
          "typescriptreact"
        ],
        "path": "./snippets/javascript/javascript.json"
      },
      {
        "language": [
          "typescriptreact"
        ],
        "path": "./snippets/javascript/react.json"
      },
      {
        "language": [
          "typescriptreact"
        ],
        "path": "./snippets/javascript/react-native.json"
      }
    ]
  }
}
